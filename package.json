{
  "name": "gramr-ts",
  "version": "1.0.9-rc16",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/caeus/gramr.git"
  },
  "homepage": "https://github.com/caeus/gramr#readme",
  "scripts": {
    "build": "tsc && tsc-alias",
    "lint": "eslint 'src/**/*.ts'",
    "format": "prettier --write 'src/**/*.ts'",
    "docs": "typedoc"
  },
  "keywords": [
    "parsing",
    "parse",
    "parsers",
    "parse combinators",
    "grammar",
    "syntax",
    "lexer",
    "tokenizer",
    "lexical analysis",
    "language",
    "lexical",
    "ast"
  ],
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "!dist/**/*.test.d.ts",
    "!dist/**/*.test.js",
    "LICENCSE",
    "README.md"
  ],
  "author": "caeus",
  "license": "MIT",
  "description": "TypeScript library designed to create typesafe, composable parsers and tokenizers for building embedded languages. With powerful parsing combinators and a focus on type safety",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "fast-check": "^3.23.1",
    "prettier": "^3.3.3",
    "tsc-alias": "^1.8.10",
    "typedoc": "^0.26.11",
    "typescript": "^5.6.2",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^2.1.3"
  }
}
